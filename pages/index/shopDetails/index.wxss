}
.add_box_ipt {
  float: left;
  width: 120rpx;
  height: 60rpx;
  font-size: 28rpx;
  color: #1f1f1f;
  text-align: center;
  line-height: 60rpx;
}
.model_style_main_box_style {
  width: 700rpx;
  margin: 0 auto;
}
.model_style_main_box_style_top {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #1f1f1f;
}
.model_style_main_box_style_box {
  padding-top: 10rpx;
  padding-bottom: 0rpx;
}
.model_style_main_box_style_box view {
  float: left;
  padding: 0 20rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  font-size: 26rpx;
  color: #1f1f1f;
  border-radius: 6rpx;
