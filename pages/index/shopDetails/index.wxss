  font-size: 32rpx;
  color: #fef9da;
  background: #546571;
}
.model_style_main_num {
  float: left;
  font-size: 32rpx;
  color: #b3b3b3;
  line-height: 80rpx;
}
.model_style_main_pri {
  float: right;
  font-size: 36rpx;
  line-height: 80rpx;
  color: #f25c5c;
}

.model_style_main_box_add {
  height: 120rpx;
  line-height: 120rpx;
