  margin: 0 auto;
  border-top: 1px solid #f0f0f0;
  padding: 20rpx 0;
}
.model_style_main_box_add_text {
  float: left;
  font-size: 28rpx;
  color: #1f1f1f;
}
.add_box {
  float: right;
  width: 245rpx;
  height: 60rpx;
  margin-top: 30rpx;
  line-height: 60rpx;
  bottom: 35rpx;
  right: 25rpx;
  border: 1px solid #cccccc;
  border-radius: 6rpx;
  overflow: hidden;
}
