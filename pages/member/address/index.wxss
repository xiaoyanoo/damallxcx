/* pages/member/address/index.wxss */
.container {
  background: #f0f0f0;
}
.addku_li {
  width: 100%;
  height: 88rpx;
  margin: 0 auto;
  line-height: 88rpx;
  background: #ffffff;
  border-bottom: 1px solid #f0f0f0;
}
.addku_li_name {
  width: 180rpx;
  float: left;
  font-size: 30rpx;
  color: #202020;
  margin-left: 25rpx;
}
.addku_li_ipt {
  float: left;
  font-size: 30rpx;
  color: #202020;
  width: 400rpx;
  height: 88rpx;
}
.addku_li_ipt input {
  padding: 0 20rpx;
  margin: 0;
  width: 100%;
  height: 100%;
}
.addku_li_next {
  width: 16rpx;
  height: 30rpx;
  float: right;
  margin-top: 27rpx;
  margin-right: 25rpx;
}
.addku_li_next image {
  width: 100%;
  height: 100%;
  float: left;
}
.address {
  width:700rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fef9da;
  background: #546571;
  border-radius: 10rpx;
  margin: 300rpx auto 0;
}