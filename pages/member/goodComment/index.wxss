/* pages/member/goodComment/index.wxss */
Page{
  background: #ffffff;
}
.comment {
  padding-bottom: 88rpx;
}
.car_box_li_goods {
  padding-top: 1rpx;
}
.car_box_li_goods_li {
  width: 100%;
  height: 210rpx;
  position: relative;
  background-color: #ffffff;
}
.car_box_li_goods_li_con_main_tit {
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  color: #1f1f1f;
  margin-top: 40rpx;
}
.car_box_li_goods_li_con_main_style {
  width: 100%;
  line-height: 36rpx;
  font-size: 24rpx;
  margin-top: 10rpx;
  color: #1f1f1f;
}
.car_box_li_goods_li_con_main_pri {
  width: 100%;
  margin-top: 10rpx;
  font-size: 36rpx;
  color: #f25c5c;
}
.logistics_top {
  width: 100%;
  height: 210rpx;
  background: #ffffff;
  border-bottom: 1rpx solid #f0f0f0;
}
.logistics_top_img {
  float: left;
  width: 150rpx;
  height: 150rpx;
  margin-top: 35rpx;
  margin-left: 25rpx;
  position: relative;
}
.logistics_top_img image{
  width: 100%;
  height: 100%;
  float: left;
  border-radius: 6rpx;
}
.logistics_top_img_msg {
  position: absolute;
  left: 0;
  bottom: 0;
  background: rgba(31,31,31,0.5);
  font-size: 22rpx;
  padding: 0 10rpx;
  color: #ffffff;
  line-height: 36rpx;
  height: 36rpx;
}
.logistics_top_main {
  width: 500rpx;
  float: left;
  height: 210rpx;
  margin-left: 25rpx;
}
.logistics_top_main_top {
  line-height: 40rpx;
  font-size: 30rpx;
  color :#1a1a1a;
  margin-top: 30rpx;
}
.logistics_top_main_top text {
  color: #ffa000;
  font-size: 24rpx;
}
.logistics_top_main_text {
  font-size: 26rpx;
  line-height: 40rpx;
  color: #7b7a7a;
  margin-top: 10rpx;
}


.order_check {
  width :100%;
  border-bottom: 1px solid #f0f0f0;
  border-top: 20rpx solid #f0f0f0;
  line-height: 88rpx;
  height: 88rpx;
  background: #ffffff;
}
.order_check_text {
  float: left;
  font-size: 28rpx;
  color: #1f1f1f;
  line-height: 88rpx;
  margin-left: 25rpx;
}
.order_check_msg {
  float: right;
  width: 500rpx;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  color: #1f1f1f;
  line-height: 88rpx;
  margin-right: 0rpx;
}
.setup_li_next {
  float: right;
  height: 88rpx;
  width: 88rpx;
  margin-right: 0rpx;
}
.setup_li_next image{
  width: 14rpx;
  height: 26rpx;
  margin-top: 31rpx;
  margin-left: 40rpx;
  float: left;
}
.order_check_img {
  width :100%;
  background: #ffffff;
}
.order_check_img .order_check_texts{
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  color: #1f1f1f;
  line-height: 88rpx;
  margin-left: 25rpx;
}
.order_check_img_li {
  float: left;
  width: 136rpx;
  height: 126rpx;
  margin-bottom: 25rpx;
  margin-left: 12rpx;
  background: #ffffff;
}
.order_check_img_li image {
  float: left;
  width: 100%;
  height: 100%;
}
.tk_btn {
  width: 700rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fef9da;
  border-radius: 6rpx;
  background: #546571;
  margin: 188rpx auto 88rpx;
}
.rank {
  width: 100%;
  height: 160rpx;
  background: #ffffff;
  border-top: 1px solid #f0f0f0;
  padding: 25rpx 0;
}
.rank_ipt {
  display: block;
  width: 700rpx;
  height: 160rpx;
  margin: 0 auto;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #1f1f1f;
}

.order_check_xx {
  float: left;
  height: 80rpx;
  margin-left: 30rpx;
}
.order_check_xx image {
  width: 35rpx;
  height: 32rpx;
  float: left;
  margin-right: 30rpx;
  margin-top: 26rpx;
}

.comment_btn {
  position: fixed;
  width: 100%;
  height: 88rpx;
  bottom: 0;
  left: 0;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #fef9da;
  text-align: center;
  background: #546571;
}