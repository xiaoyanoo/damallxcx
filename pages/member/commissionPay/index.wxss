/* pages/member/commissionPay/index.wxss */
Page{
  background: #ffffff;
}
.commissionPay_tit {
  color: #1f1f1f;
  font-size: 32rpx;
  line-height: 48rpx;

  height: 88rpx;
  padding-left: 25rpx;
  padding-top: 50rpx;
}
.commissionPay_ipt {
  width: 700rpx;
  margin: 20rpx auto 50rpx;
  height: 160rpx;
  border: 1px solid #cccccc;
}
.commissionPay_ipt_text {
  float: left;
  line-height: 160rpx;
  margin-left: 25rpx;
}
.commissionPay_num {
  float: left;
  width: 600rpx;
  margin-left: 35rpx;
  height: 80rpx;
  font-size: 72rpx;
  color: #f25c5c;
  margin-top: 40rpx;
}





.order_check_img {
  width :100%;
  background: #ffffff;
}
.order_check_img .order_check_texts{
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 32rpx;
  color: #1f1f1f;
  line-height: 88rpx;
  margin-left: 25rpx;
}
.order_check_img_li {
  float: left;
  width: 136rpx;
  height: 126rpx;
  margin-bottom: 25rpx;
  margin-left: 12rpx;
  background: #ffffff;
}
.order_check_img_li image {
  float: left;
  width: 100%;
  height: 100%;
}

.comment_btn {
  position: fixed;
  width: 700rpx;
  left: 25rpx;
  height: 88rpx;
  bottom: 200rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #fef9da;
  text-align: center;
  background: #546571;
  border-radius: 6rpx;
}